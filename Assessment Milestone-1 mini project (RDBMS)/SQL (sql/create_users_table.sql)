CREATE TABLE USERS (
USERID VARCHAR2(200) PRIMARY KEY,
PASSWORD VARCHAR2(200),
NAME VARCHAR2(200),
INCORRECTATTEMPTS NUMBER(2),
LOCKSTATUS NUMBER(2),
USERTYPE VARCHAR2(200)
);




